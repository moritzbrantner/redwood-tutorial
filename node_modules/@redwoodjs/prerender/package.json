    "babel-plugin-ignore-html-and-css-imports": "0.1.0",
    "cheerio": "1.0.0-rc.11",
    "core-js": "3.22.8",
    "cross-undici-fetch": "0.1.27",
    "mime-types": "2.1.35"
  },
  "devDependencies": {
    "@babel/cli": "7.16.7",
    "@babel/core": "7.16.7",
    "@types/mime-types": "2.1.1",
